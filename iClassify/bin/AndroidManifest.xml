<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capstoneii.iclassify"
    android:versionCode="3"
    android:versionName="1.0.2" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
 	  <supports-screens android:smallScreens="false"
                      android:normalScreens="false"
                      android:largeScreens="true"
                      android:xlargeScreens="true"
                      android:requiresSmallestWidthDp="600" />
   
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
      
        <activity
            android:name="com.capstoneii.iclassify.SplashScreenActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
             >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
         <activity
            android:name="com.capstoneii.iclassify.viewpager.ViewPagerIntro"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
             >
          </activity> 
        
		<activity
            android:name="drawer.MainDrawerActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light"
            android:screenOrientation="portrait" >
        </activity> 
        
        <activity android:name="descisiondiscussflip.DescTreeLayoutActivity"
            android:theme="@style/Theme.AppCompat.Light"
            android:screenOrientation="portrait"/>
        
          <activity android:name="knearestdiscussflip.KNearestLayoutActivity"
            android:theme="@style/Theme.AppCompat.Light"
            android:screenOrientation="portrait"/>
          
          <activity android:name="bayesdiscussflip.NativeBayesLayoutActivity"
            android:theme="@style/Theme.AppCompat.Light"
            android:screenOrientation="portrait"/>
          
        
          //Simulation
           <activity android:name="com.capstoneii.iclassify.simulation.desiciontree.SimulStartActivity"
           	android:screenOrientation="portrait"/>
           
            <activity android:name="com.capstoneii.iclassify.simulation.desiciontree.SimulProcessIntroActivity"
           	android:screenOrientation="portrait"/>
          
           <activity android:name="com.capstoneii.iclassify.simulation.desiciontree.SimulProblemActivity"
           	android:screenOrientation="portrait"/>
           
           <activity android:name="com.capstoneii.iclassify.simulation.desiciontree.SimulDatasetActivity"
           	/>
           
     //Video
     
      <activity android:name="com.capstoneii.iclassify.videos.VideoMenuActivity"
           android:label="Videos"
           android:theme="@style/Theme.AppCompat.Light"
           	/>
     <activity android:name="com.capstoneii.iclassify.videos.VideoDecisionTree"
           	/>
      <activity android:name="com.capstoneii.iclassify.videos.VideoKNearestActivity"
           	/>
      <activity android:name="com.capstoneii.iclassify.videos.VideoNAtiveBayesianActivity"
           	/>
      
      <activity android:name="com.capstoneii.iclassify.videos.IntroductionVideo"
            android:theme="@style/Theme.AppCompat.Light"
           	/>
      
      
      //Assessment
       <activity android:name="com.capstoneii.iclassify.assessment.decisionid3.DecisionTreeAssessmentActivity"
           	android:theme="@style/Theme.AppCompat.Light"
           	/>
        <activity android:name="com.capstoneii.iclassify.assessment.decisionid3.DecisionTreeAssessmentDynamicFirstQuestionActivity"
           	android:theme="@style/Theme.AppCompat.Light"
           	/>
        
         <activity android:name="com.capstoneii.iclassify.assessment.decisionid3.DecisionTreeAssessmentJumbleWord"
           	android:theme="@style/Theme.AppCompat.Light"
           	/>
         
         <activity android:name="com.capstoneii.iclassify.assessment.decisionid3.DecisionTreeQuestionSets"
           	android:theme="@style/Theme.AppCompat.Light"
           	/>
       
       <activity android:name="com.capstoneii.iclassify.assessment.knn.KnnAssessmentActivity"
           	android:theme="@style/Theme.AppCompat.Light"
           	/>
         <activity android:name="com.capstoneii.iclassify.assessment.bayesian.BayesianAssessmentActivity"
           	android:theme="@style/Theme.AppCompat.Light"
           	/>
        <activity android:name="com.capstoneii.iclassify.problems.SimpsonsDecisionTreeFragment"
           	android:theme="@style/Theme.AppCompat.Light"
           	/>
       
        
        <activity android:name="com.capstoneii.iclassify.QuizResultActivity"
           	android:theme="@style/Theme.AppCompat.Light"
           	/>
        
    </application>
    
   
</manifest>



